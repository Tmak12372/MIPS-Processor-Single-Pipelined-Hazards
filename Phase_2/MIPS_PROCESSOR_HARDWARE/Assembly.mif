DEPTH = 256;                   -- The size of memory in words
WIDTH = 32;                    -- The size of data in bits
ADDRESS_RADIX = HEX;          -- The radix for address values
DATA_RADIX = BIN;             -- The radix for data values
CONTENT                       -- start of (address : data pairs)
BEGIN

[00..FF]:00000000000000000000000000000000;

00 : 00100000000010000000000000000010; --addi $t0, $zero, 2
04 : 00100000000010010000000000000010; --addi $t1, $zero, 2
08 : 00100000000100000000000000001100; --addi $s0, $zero, 12
0C : 00100000000100010000000000001000; --addi $s1, $zero, 8
10 : 00100000000100110000000000001111; --addi $s3, $zero, 15
14 : 00000001000010010101100000100000; --add $t3, $t0, $t1
18 : 00000010000100010110000000100010; --sub $t4, $s0, $s1
1C : 00000001011011000110100000100100; --and $t5, $t3, $t4
20 : 00000010011101000100000000101010; --xor $t0, $s3, $s4
24 : 00000001000111110100100000100111; --nor $t1, $t0, $RA
28 : 00000000000100000101000100000000; --sll $t2, $s0, 4
2C : 00100010011100110000000000000100; --addi $s3, $s3, 4
END;