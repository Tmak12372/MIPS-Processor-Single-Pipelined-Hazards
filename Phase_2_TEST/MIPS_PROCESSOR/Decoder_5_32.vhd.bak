    OUTPUT <= "00000000000000000000000010000000" WHEN INPUT = "00111"; --8
    OUTPUT <= "00000000000000000000000100000000" WHEN INPUT = "01000"; --9
    OUTPUT <= "00000000000000000000001000000000" WHEN INPUT = "01001"; --10
    OUTPUT <= "00000000000000000000010000000000" WHEN INPUT = "01010"; --11
    OUTPUT <= "00000000000000000000100000000000" WHEN INPUT = "01011"; --12
    OUTPUT <= "00000000000000000001000000000000" WHEN INPUT = "01100"; --13
    OUTPUT <= "00000000000000000010000000000000" WHEN INPUT = "01101"; --14
    OUTPUT <= "00000000000000000100000000000000" WHEN INPUT = "01110"; --15
    OUTPUT <= "00000000000000001000000000000000" WHEN INPUT = "01111"; --16
    OUTPUT <= "00000000000000010000000000000000" WHEN INPUT = "10000"; --17
    OUTPUT <= "00000000000000100000000000000000" WHEN INPUT = "10001"; --18
    OUTPUT <= "00000000000001000000000000000000" WHEN INPUT = "10010"; --19
    OUTPUT <= "00000000000010000000000000000000" WHEN INPUT = "10011"; --20
    OUTPUT <= "00000000000100000000000000000000" WHEN INPUT = "10100"; --21
    OUTPUT <= "00000000001000000000000000000000" WHEN INPUT = "10101"; --22
    OUTPUT <= "00000000010000000000000000000000" WHEN INPUT = "10110"; --23
    OUTPUT <= "00000000100000000000000000000000" WHEN INPUT = "10111"; --24
    OUTPUT <= "00000001000000000000000000000000" WHEN INPUT = "11000"; --25
    OUTPUT <= "00000010000000000000000000000000" WHEN INPUT = "11001"; --26
    OUTPUT <= "00000100000000000000000000000000" WHEN INPUT = "11010"; --27
    OUTPUT <= "00001000000000000000000000000000" WHEN INPUT = "11011"; --28
    OUTPUT <= "00010000000000000000000000000000" WHEN INPUT = "11100"; --29
    OUTPUT <= "00100000000000000000000000000000" WHEN INPUT = "11101"; --30
    OUTPUT <= "01000000000000000000000000000000" WHEN INPUT = "11110"; --31
    OUTPUT <= "10000000000000000000000000000000" WHEN INPUT = "11111"; --32
END RTL;




