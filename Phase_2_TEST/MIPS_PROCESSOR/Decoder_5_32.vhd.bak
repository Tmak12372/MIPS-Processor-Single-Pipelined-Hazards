LIBRARY  IEEE;
USE IEEE.std_logic_1164.all;
USE IEEE.numeric_std.all;

ENTITY Decoder_5_32 IS 
    PORT (INPUT : IN STD_LOGIC_VECTOR(4 DOWNTO 0);
          OUTPUT : OUT STD_LOGIC_VECTOR(31 DOWNTO 0)
    );
END Decoder_5_32;

ARCHITECTURE RTL OF Decoder_5_32 IS
BEGIN
    OUTPUT <= "00000000000000000000000000000001" WHEN INPUT = "00000"; --1
    OUTPUT <= "00000000000000000000000000000010" WHEN INPUT = "00001"; --2
    OUTPUT <= "00000000000000000000000000000100" WHEN INPUT = "00010"; --3
    OUTPUT <= "00000000000000000000000000001000" WHEN INPUT = "00011"; --4
    OUTPUT <= "00000000000000000000000000010000" WHEN INPUT = "00100"; --5
    OUTPUT <= "00000000000000000000000000100000" WHEN INPUT = "00101"; --6
